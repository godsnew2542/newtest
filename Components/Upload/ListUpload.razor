@page "/ListUpload";

<div>
    <div class="row my-3">
        <div class="col-xxl-6 col-xl-8 col-lg-8 col-md-8 col-sm-7">
            <div class="px-3">
                <i class="fa-solid fa-paperclip mx-2 " style="color:red"></i>
                @listModel.Name
            </div>
        </div>
        <div class="col-xxl-6 col-xl-4 col-lg-4 col-md-4 col-sm-5 text-start">
            <button type="button"
                    class="btn error-color"
                    style="border:solid 1px;"
                    @onclick="()=>ConfirmFromAsync(listModel)">
                <i class="fas fa-trash-alt"></i>
            </button>
        </div>
    </div>
</div>

